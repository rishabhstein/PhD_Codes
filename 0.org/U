FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    inlet
    {
        type            pressureInletVelocity;
        phi             phi;
        value           uniform (1 0 0);
    }
    outlet
    {
        type            pressureInletVelocity;
        phi             phi;
        value           uniform (1 0 0);
    }
    y1
    {
        type    cyclic;
    }
    y2
    {
        type    cyclic;
    }
    z1
    {
        type    empty;
    }
    z2
    {
        type    empty;
    }
}


// ************************************************************************* //
