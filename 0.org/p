FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            fixedValue;
        value           uniform -1;
    }
    y1
    {
        type            cyclic;
    }
    y2
    {
        type            cyclic;
    }
    z1
    {
        type            empty;
    }
    z2
    {
        type            empty;
    }
}


